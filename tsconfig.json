{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "es6",
        "sourceMap": true,
        "declaration": false,
        "forceConsistentCasingInFileNames": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "resolveJsonModule": true
    },
    "include": [
        "./src/ts/index.ts"
    ]
}